#include <iostream>

using namespace std;
template <class T>
T sum (T a,T b)
{
    if(b!=0){
 return a/b;
 }
}
int main()
{
    cout<<sum<float>(10.6f,20.7f)<<endl;
    cout<<sum<float>(10,20)<<endl;
     /*cout<<sum<bool>(true,false)<<endl;
    cout<<sum<char>('a','b')<<endl;
     cout<<sum<string>("Ala","Kot")<<endl;*/
}
